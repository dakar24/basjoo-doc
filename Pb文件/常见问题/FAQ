package com.qjoy.basjoo.core.model.pb;

option java_package="com.qjoy.basjoo.core.model.pb";


//获取常见问题列表
message GetFAQListRequest {   

  //起始index
  optional int32 startIndex = 1;  
 
    
}

//获取常见问题结果
message GetFAQListResult { 
 
   required bool                   success = 1;

   required string                 resultCode = 2;
    
   required string                 resultMsg = 3;
   
   //是否有更多
   required bool                   hasMore = 4;
   
   //当前最大index
   optional int32                  maxIndex = 5;
   
   //常见问详细内容
   repeated FAQInfo               FAQInfos = 5;
   
}

//FAQ信息
message FAQInfo{
   
   //问题
   required  string  question = 1;
   
   //答案，每列强制换行
   repeated  string  answer = 2;      
}

